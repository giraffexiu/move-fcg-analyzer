{
  "name": "move-fcg-analyzer",
  "version": "1.0.8",
  "description": "A tree sitter parser for Move language with function call graph analysis.",
  "main": "index.js",
  "types": "bindings/node",
  "bin": {
    "move-fcg-analyzer": "./dist/src/cli.js"
  },
  "scripts": {
    "test": "tree-sitter test",
    "parse": "tree-sitter parse",
    "generate": "tree-sitter generate",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "format": "eslint --fix grammar.js",
    "format:check": "eslint grammar.js",
    "build:indexer": "tsc",
    "indexer": "npm run build:indexer && node dist/src/cli.js"
  },
  "author": "ArArgon <liaozping@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.22.6",
    "typescript": "^5.9.3"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "tree-sitter": [
    {
      "scope": "source.move",
      "injection-regex": "^move$"
    }
  ]
}
